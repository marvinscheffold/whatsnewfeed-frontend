{
    "name": "react-typescript-boilerplate",
    "version": "1.0.0",
    "description": "React + TypeScript boilerplate on the shoulders of giant parcel",
    "scripts": {
        "start": "parcel serve src/index.html --dist-dir dist/dev",
        "build": "parcel build src/index.html --dist-dir dist/build && npm run copy",
        "copy": "cp static/_redirects dist/build",
        "serve:build": "npx http-server dist/build"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@react-hook/window-size": "^3.0.7",
        "antd": "^4.20.5",
        "parcel": "^2.5.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-image": "^4.0.3",
        "react-loading-skeleton": "^3.1.0",
        "react-qr-code": "^2.0.7",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "socket.io-client": "^4.5.1"
    },
    "devDependencies": {
        "@parcel/transformer-less": "^2.5.0",
        "buffer": "^6.0.3",
        "process": "^0.11.10"
    }
}
